# .env.example

# Replace with your real Discord bot token
DISCORD_TOKEN=your_discord_bot_token_here

# MongoDB connection string (use Atlas or localhost)
MONGO_URI=mongodb://localhost:27017

# Your MongoDB database name
MONGO_DB_NAME=your_database_name_here

# API key for bracket system or any 3rd-party service
BRACKET_API_KEY=your_bracket_api_key_here

# Username for bracket API
BRACKET_API_USERNAME=your_bracket_api_username_here

# Discord Application ID (from Developer Portal)
Application_ID=your_discord_application_id_here

# Public key for interactions (from Developer Portal)
Public_Key=your_discord_public_key_here
